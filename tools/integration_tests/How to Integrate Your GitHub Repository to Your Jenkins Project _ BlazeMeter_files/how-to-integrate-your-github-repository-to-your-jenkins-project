jQuery.each(jQuery(".author_blog_taxonomy_hidden_name"), function (index, value) {
author_name = jQuery(value).text();
taxonomy_id = jQuery(value).attr("author_taxonomy_id");
var html = jQuery(".blog_author_block span:contains(" + author_name + ")").clone().html();
html = "<a class='new_blog_author_name' href='/blogs-on-author/" + taxonomy_id + "'>" + html + "</a>";
jQuery(".blog_author_block span:contains(" + author_name + ") ").html(html);
});
jQuery("#hidden_authors").remove();
</script>
