jQuery.each(jQuery(".author_blog_taxonomy_hidden_name"), function (index, value) {
author_name = jQuery(value).text();
taxonomy_id = jQuery(value).attr("author_taxonomy_id");
var html = jQuery(".node-blog-author .new_blog_top_author_photo_name:contains(" + author_name + ")").clone().html();
html = "<a href='/blogs-on-author/" + taxonomy_id + "'>" + html + "</a>";
jQuery(".node-blog-author .new_blog_top_author_photo_name:contains(" + author_name + ")").html(html);
});
</script>
