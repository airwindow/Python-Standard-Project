<script type="text/javascript">
jQuery.each(jQuery('p.author_blog_hidden_name'), function (index, value) {
author_name = false;
if (jQuery(value).text().length != 0) {
author_name = jQuery(value).text();
container = jQuery(value);
}
if (author_name) {
jQuery(".node-blog-author:contains('" + author_name + "')").find("img").attr("src", jQuery(container).parent().find("img").attr('src'));
//			jQuery(".post_view_about_author:contains('"+author_name+"')").find("img").attr("src",jQuery(container).parent().find("img").attr('src'));
}
});
</script>
